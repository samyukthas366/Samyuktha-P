class Node:
    def __init__(self,data):
        self.data=data
        self.next=None
head=Node(10)
head.next=Node(20)
head.next.next=Node(30)
head.next.next.next=Node(40)
head.next.next.next.next=Node(60)
temp=head
while temp:
    print(temp.data,"-->",end=" ")
    temp=temp.next
print("none")

