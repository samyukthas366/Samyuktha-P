class Node(object):
    def __init__(self,data):
        self.data=data
        self.prev=None
        self.next=None
class Dll(object):
    def __init__(self):
        self.head=None
    def treversal(self):
        temp=self.head
        while temp:
           print(temp.data,"__>",end=" ")
           if(temp.next==None):
               last=temp
           temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
