class Node(object):
    def __init__(self,data):
        self.prev=None
        self.data=data
        self.next=None
class Dll(object):
    def __init__(self):
        self.head=None
    def traversal(self):
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            temp=temp.next
        print("None")
        temp=self.head
        if(not temp):
            return
        while temp.next:
            temp=temp.next
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
head=Node(1000)
