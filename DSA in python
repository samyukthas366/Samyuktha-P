    def __init__(self):
        self.head=None
    def finding_position(self,key):
        temp=self.head
        position=0
        Found=False
        while(temp):
            if(temp.data==key):
                print(f"the value is {key} and its position is {position}")
                Found=True
            temp=temp.next
            position+=1
        if(not Found):
           print("not found")
