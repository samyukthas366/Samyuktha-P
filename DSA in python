class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
        self.prev=None
class Dll(object):
    def __init__(self):
        self.head=None
    def traversal(self):
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            if(temp.next==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
    def inst_begin(self,data):
        temp=self.head
        new_node=Node(data)
        new_node.next=self.head
        self.head.prev=new_node
        self.head=new_node
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            if(temp.next==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
    def inst_middle(self,data):
        temp=self.head
        new_node=Node(data)
        new_node.next=self.head.next
        self.head.next.prev=new_node
        new_node.prev=self.head
        self.head.next=new_node
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            if(temp.next==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
    def inst_end(self,data):
        temp=self.head
        new_node=Node(data)
        if(temp.data==None):
            return
        if(temp.next==None):
            return
        temp=self.head
        while temp.next:
            temp=temp.next
        temp.next=new_node
        new_node.prev=temp
        new_node.next=None
        temp=self.head
        while(temp):
            print(temp.data,"__>",end=" ")
            if(temp.next==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
