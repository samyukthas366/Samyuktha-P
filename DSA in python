class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
        self.prev=None
class Dll(object):
    def __init__(self):
        self.head=None
    def delete_middle(self):
        temp=self.head
        if(temp.data==None):
            return
        self.head.next.next=self.head.next.next.next
        self.head.next.next.prev=self.head.next
        temp=self.head
        while temp:
            print(temp.data,"<__",end=" ")
            if(temp.next==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
