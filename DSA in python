class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
        self.prev=None
class Dll(object):
    def __init__(self):
        self.head=None
    def delete_begin(self):
        temp=self.head
        self.head=self.head.next
        self.head.prev=None
        temp=self.head
        result=[]
        while(temp):
            result.append(str(temp.data))
            temp=temp.next
        print("<__>".join(result),"<__>None")
    def delete_middle(self):
        temp=self.head
        self.head.next.next=self.head.next.next.next
        self.head.next.next.prev=self.head.next
        temp=self.head
        result = []
        while (temp):
            result.append(str(temp.data))
            temp = temp.next
        print("<__>".join(result), "<__>None")
    def delete_end(self):
        temp=self.head
        if(temp.data==None):
            return
        if(temp.next==None):
            return
        while temp.next.next:
            temp=temp.next
        temp.next=None
        temp=self.head
        result = []
        while (temp):
             result.append(str(temp.data))
             temp = temp.next
        print("<__>".join(result), "<__>None")
    def position(self,key):
        temp=self.head
        positio=0
        found=False
        while temp:
            if(temp.data==key):
                print(f"the key is {key} and its positio is{positio}")
                found=True
                return
            temp=temp.next
            positio+=1
        if(not found):
            print("not found")
head=Node(101)
head.next=Node(202)
head.next.prev=head
head.next.next=Node(303)
head.next.next.prev=head.next
head.next.next.next=Node(404)
head.next.next.next.prev=head.next.next
head.next.next.next.next=Node(505)
head.next.next.next.next.prev=head.next.next.next
head.next.next.next.next.next=Node(606)
head.next.next.next.next.next.prev=head.next.next.next.next
d=Dll()
d.head=head
d.delete_begin()
d.delete_middle()
d.delete_end()
d.position(303)

