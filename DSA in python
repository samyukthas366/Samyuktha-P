class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
head=Node(10)
head.next=Node(20)
head.next.next=Node(30)
head.next.next.next=Node(40)
new_node=Node(300)
temp=head
new_node.next=head
head=new_node
temp=head
while temp:
    print(temp.data,"-->",end=" ")
    temp=temp.next
print("none")

