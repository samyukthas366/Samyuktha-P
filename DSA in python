class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
        self.prev=None
class Dll(object):
    def __init__(self):
        self.head=None
    def traversal(self):
        temp=self.head
        result = []
        while temp:
            result.append(str(temp.data))
            temp = temp.next
        print("<->".join(result))
    def insert_begin(self,data):
        temp=self.head
        new_node=Node(data)
        new_node.next=self.head
        self.head.prev=new_node
        self.head=new_node
        temp=self.head
        result=[]
        while temp:
            result.append(str(temp.data))
            temp=temp.next
        print("<->".join(result))
    def insert_middle(self,data):
        temp=self.head
        new_node=Node(data)
        new_node.next=self.head.next.next
        self.head.next.next.prev=new_node
        new_node.prev=self.head.next
        self.head.next.next=new_node
        temp=self.head
        result=[]
        while temp:
            result.append(str(temp.data))
            temp=temp.next
        print("<-->".join(result))
    def insert_end(self,data):
        temp=self.head
        new_node=Node(data)
        if(temp.data==None):
            return
        if(temp.next==None):
            return
        while temp.next:
            temp=temp.next
        temp.next=new_node
        new_node.prev=temp
        new_node.next=None
        temp=self.head
        result = []
        while temp:
            result.append(str(temp.data))
            temp = temp.next
        print("<->".join(result),"<->None")

