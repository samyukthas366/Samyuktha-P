
####STACK IN PYTHON###
class stack:
    def __init__(self):
        self.stack=[]
        self.indexofstack=0
    def isEmpty(self):
        return self.stack==[]
    def push(self,data):
        self.stack.insert(self.indexofstack,data)
        self.indexofstack+=1
        return f"the {self.stack} element is been added"
    def pop(self):
        self.indexofstack-=1
        return self.stack.pop(self.indexofstack)
    def size11(self):
        return len(self.stack)
a=__name__=="__main__"
s=stack()
print(s.isEmpty())
print(s.push(100))
print(s.push(200))
#print(s.pop())
print(s.pop())
print(s.size11())


class stack123:
    def __init__(self):
        self.stack=[]
        self.index=0
    def push(self,data):
        self.stack.insert(0,data)
        self.index+=1
        return f"the {self.stack} element is been added"
    def pop(self):
        self.index-=1
        return self.stack.pop(self.index)
    def size(self):
        return len(self.stack)
a=__name__=="__main__"
s=stack()
print(s.push(100))
print(s.pop())

class stack1(object):
    def __init__(self):
        self.stack=[]
        self.indesnumber=0
    def Empty(self):
        return self.stack==[]
    def isEmpty(self):
        return self.stack==[]
a=__name__=="main__"
s=stack1()
print(s.Empty())
print(s.isEmpty())


class stack2():
    def __init__(self):
        self.stack=[]
        self.indexis=0
    def push(self,data):
        self.stack.append(data)
        self.indexis+=1
        return "{} is the number is pushed in tho the stack".format(data)
s=__name__=="__main__"
s2=stack2()
print(s2.push(100))
print(s2.push(900))

