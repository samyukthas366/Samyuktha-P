class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
        self.prev=None
class Dll(object):
    def __init__(self):
        self.head=None
    def delete_begin(self):
        temp=self.head
        if(temp.data==None):
            return
        self.head=self.head.next
        self.head.prev=None
        temp=self.head
        while temp:
            print(temp.data,"<__",end=" ")
            if(temp.next==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
