class st:
    def __init__(self):
        self.stack=[]
        self.index=0
    def isEmpty(self):
        return self.stack==[]
    def push(self,data):
        self.stack.insert(self.index,data)
        self.index+=1
        return "{} is the stack ".format(data)
    def pop(self):
        self.index-=1
        return self.stack.pop(self.index)
    def size(self):
        return len(self.stack)
s=st()
print(s.isEmpty())
print(s.push(100))
print(s.pop())
print(s.size())


class names:
    def __init__(self):
        self.str=[]
        self.index=0
    def push(self,data):
        self.str.insert(self.index,data)
        self.index+=1
        return "{} is the pushed data".format(data)
    def pop(self):
        self.index-=1
        return self.str.pop(self.index)
    def size(self):
        return len(self.str)
if __name__=="__main__":
  a=names()
  print(a.push(100))
  print(a.pop())
  print(a.size())


class stack:
    def __init__(self):
        self.str=[]
        self.index=0
    def isEmpty(self):
        return self.str==[]
    def push(self,data):
        self.str.insert(self.index,data)
        self.index+=1
        return "{} is the appened or pushed element in the str".format(data)
    def pop(self):
        self.index-=1
        return self.str.pop(self.index)
    def top(self):
        return self.str[self.index]
    def size(self):
        return len(self.str)
if __name__=="__main__":
    s=stack()
    print(s.isEmpty())
    print(s.push(100))
    print(s.push(200))
    print(s.push(300))
    print(s.pop())
    print(s.pop())
    print(s.size())

class stack:
    def __init__(self):
        self.str=[]
        self.index=0
    def push(self,data):
        self.str.insert(self.index,data)
        self.index+=1
        return "{} is the push element".format(data)
    def pop(self):
        self.index-=1
        self.str.pop(self.index)
        return self.str.pop(self.index)
    def size(self):
        return len(self.str)
if __name__=="__main__":
    s=stack()
    print(s.size())
    print(s.push(100))
    print(s.push(200))
    print(s.push(400))
    print(s.push(800))
    print(s.size())


class names:
    def __init__(self):
        self.stack=[]
        self.indexing=0
    def isEmpty(self):
        return self.stack==[]
    def push(self,data):
        self.stack.insert(self.indexing,data)
        self.indexing+=1
        return "{} is the element in the stack".format(data)
    def pop(self):
        self.indexing-=1
        return self.stack.pop(self.indexing)
    def size(self):
        return len(self.stack)
if __name__=="__main__":
    s=names()
    print(s.push("sam"))
    print(s.push("lahari"))
    print(s.push("srinivas"))
    print(s.push("sunitha"))
    print(s.push("anasuya"))
    print(s.pop())
    print(s.pop())
    print(s.size())
    print(s.stack)


class names:
    def __init__(self):
        self.stack=["sam","lah"]
        self.indexing=0
    def isEmpty(self):
        return self.stack==[]
    def push(self,data):
        self.stack.append(data)
        self.indexing+=1
        return "{} is the data ".format(data)
    def pop(self):
        self.indexing-=0
        return self.stack.pop(self.indexing)
    def size(self):
        return len(self.stack)
s=names()
print(s.size())
print(s.push("jimin"))
print(s.push("v"))
print(s.push("suga"))
print(s.stack)
print(s.pop())
print(s.pop())
print(s.stack)
print(s.size())

class stack(object):
    def __init__(self):
        self.stack=[]
        self.index=0
    def push(self,data):
        self.stack.insert(self.index,data)
        self.index+=1
        return "{} is the data added in stack".format(data)
    def pop(self):
        self.index-=1
        return self.stack.pop(self.index)
    def size(self):
        return len(self.stack)
if __name__=="__main__":
    s=stack()
    print(s.push(100))
    print(s.push("sam"))
    print(s.push("jimina"))
    print(s.push("v"))
    print(s.pop())
    print(s.pop())
    print(s.size())
    print(s.stack)
    print(s.size())

#####queas
class ques(object):
    def __init__(self):
        self.ques=[]
    def enques(self,data):
        self.ques.insert(0,data)
        return "{} is the data in stack".format(data)
    def deques(self):
        self.ques.pop()
    def size(self):
        return len(self.ques)
if __name__=="__main__":
    s=ques()
    print(s.enques(100))
    print(s.enques(200))
    print(s.enques("sam"))
    print(s.enques("lahari"))
    print(s.deques())
    print(s.deques())
    print(s.size())
    print(s.ques)


class quesss:
     def __init__(self):
        self.ques=[]
        def enques(self,data):
            self.ques.insert(0,data)
            return "{} is the data".format(data)
        def deques(self):
            self.ques.pop()
            return self.ques.pop()
        def size(self):
            return len(self.ques)
if __name__=="__main__":
    s=quesss()
    #print(s.size())
    #print(s.enques(100))
    #print(s.deques())

class ques:
    def __init__(self):
        self.ques=[]
    def isEmpty(self):
        return self.ques==[]
    def enques(self,data):
        self.ques.insert(0,data)
        return "{} is the data in ques".format(data)
    def deques(self):
        self.deques.pop()
        return self.ques.pop()
    def size(self):
        return len(self.ques)
if __name__=="__main__":
    q=ques()
    print(q.size())
    print(q.enques(100))
    print(q.enques(200))
    print(q.enques("sam"))
    print(q.enques("lahari"))
    print(q.deques())
    print(q.deques())
    print(q.size())

