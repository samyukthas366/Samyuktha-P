class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
head=Node(10)
head.next=Node(20)
head.next.next=Node(30)
head.next.next.next=Node(40)
head.next.next.next.next=Node(50)
head.next.next.next.next.next=Node(60)
temp=head
key=30
position=0
Found=False
temp=head
while temp:
    if(temp.data==key):
        print(f"the key is {key} and its position is {position}")
        Found=True
    temp=temp.next
    position+=1

>>>>>>>>>>..
class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
class Sll(object):
    def __init__(self):
        self.head=None
    def inst_begin(self,data):
        new_node=Node(data)
        temp=self.head
        new_node.next=self.head
        self.head=new_node
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            temp=temp.next
        print("None")
    def inst_middle(self,data):
        new_node=Node(data)
        temp=self.head
        new_node.next=self.head.next.next
        self.head.next.next=new_node
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            temp=temp.next
        print("None")
    def inst_end(self,data):
        new_node=Node(data)
        temp=self.head
        while temp.next:
            temp=temp.next
        temp.next=new_node
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            temp=temp.next
        print("None")
head=Node(10)
head.next=Node(20)
head.next.next=Node(30)
head.next.next.next=Node(40)
head.next.next.next.next=Node(50)
head.next.next.next.next.next=Node(60)
head.next.next.next.next.next.next=Node(70)
s=Sll()
s.head=head
s.inst_begin(1234)
s.inst_middle(123456)
s.inst_end(90786)

>>>>>>>>>>>>
class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
class LinkedList(object):
    def __init__(self):
        self.head=None
    def dele_begin(self):
        temp=self.head
        self.head=self.head.next
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            temp=temp.next
        print("None")
    def dele_middle(self):
        temp=self.head
        self.head.next=self.head.next.next
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            temp=temp.next
        print("None")
    def dele_end(self):
        if(self.head is None):
            return
        if(self.head.next is None):
            self.head=None
            return
        temp=self.head
        while(temp.next.next):
            temp=temp.next
        temp.next=None
        temp=self.head
        while temp:
           print(temp.data, "__>", end=" ")
           temp = temp.next
        print("None")
head=Node(10)
head.next=Node(20)
head.next.next=Node(30)
head.next.next.next=Node(40)
head.next.next.next.next=Node(50)
head.next.next.next.next.next=Node(60)
head.next.next.next.next.next.next=Node(70)
head.next.next.next.next.next.next.next=Node(80)
head.next.next.next.next.next.next.next.next=Node(90)
s=LinkedList()
s.head=head
s.dele_begin()
s.dele_middle()
s.dele_end()

####position#####
#################
class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
class Sll(object):
    def __init__(self):
        self.head=None
    def finding_position(self,key):
        temp=self.head
        position=0
        Found=False
        while(temp):
            if(temp.data==key):
                print(f"the value is {key} and its position is {position}")
                Found=True
            temp=temp.next
            position+=1
        if(not Found):
           print("not found")
head=Node(10)
head.next=Node(20)
head.next.next=Node(30)
head.next.next.next=Node(40)
head.next.next.next.next=Node(50)
head.next.next.next.next.next=Node(60)
head.next.next.next.next.next.next=Node(70)
s=Sll()
s.head=head
s.finding_position(30)

