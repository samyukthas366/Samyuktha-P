        while temp:
            print(temp.data,"__>",end=" ")
            if(temp.next==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
    def inst_begin(self,data):
        new_node=Node(data)
        temp=self.head
        new_node.next=self.head
        self.head.prev=new_node
        self.head=new_node
        temp=self.head
        while temp:
            print(temp.data,"<__",end=" ")
            if(temp.next==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
    def inst_middle(self,data):
        new_node=Node(data)
        temp=self.head
        new_node.next=self.head.next
        self.head.next.prev=new_node
        self.head.next=new_node
        new_node.prev=self.head
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            if(temp.next==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
    def instt_begin(self,data):
        new_node=Node(data)
        temp=self.head
        new_node.next=self.head.next.next
        head.next.next.prev=new_node
        new_node.prev=self.head.next
        self.head.next.next=new_node
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            if(temp.next==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
    def instr_middle(self,data):
        temp=self.head
        new_node=Node(data)
        new_node.next=self.head.next.next.next
        self.head.next.next.next.prev=new_node
        new_node.prev=self.head.next.next
        self.head.next.next.next=new_node
        temp=self.head
        while temp:
