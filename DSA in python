class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
head=Node(100)
head.next=Node(200)
head.next.next=Node(300)
head.next.next.next=Node(400)
head.next.next.next.next=Node(500)
new_node=Node(1000)
temp=head
while temp.next:
    temp=temp.next
temp.next=new_node
temp=head
while temp:
    print(temp.data,"-->",end=" ")
    temp=temp.next
print("none")

