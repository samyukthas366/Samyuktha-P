head=Node(10)
head.next=Node(20)
head.next.prev=head
head.next.next=Node(30)
head.next.next.prev=head.next
head.next.next.next=Node(40)
head.next.next.next.prev=head.next.next
head.next.next.next.next=Node(50)
head.next.next.next.next.prev=head.next.next.next
d=Dll()
d.head=head
d.instr_middle(900000)
d.inst_middle11(567890)

>>>>>>>>>>>
class Node(object):
    def __init__(self,data):
        self.data=data
        self.next=None
        self.prev=Node
class Dll(object):
    def __init(self):
        self.head=None
    def traversal(self):
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            if(temp.next==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
    def inst_begin(self,data):
        temp=self.head
        new_node=Node(data)
        new_node.next=self.head
        self.head.prev=new_node
        self.head=new_node
        temp=self.head
        while temp:
            print(temp.data,"__>",end=" ")
            if(temp.data==None):
                last=temp
            temp=temp.next
        print("None")
        temp=last
        while temp:
            print(temp.data,"<__",end=" ")
            temp=temp.prev
        print("None")
head=Node(10)
head.next=Node(20)
head.next.prev=head
head.next.next=Node(30)
head.next.next.prev=head.next
head.next.next.next=Node(40)
head.next.next.next.prev=head.next.next
d=Dll()
d.head=head
d.traversal()
d.inst_begin(900000)














