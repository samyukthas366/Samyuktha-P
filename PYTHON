class ClassName():
    followers=10
    def __init__(self,name,course):
        self.names=name
        self.courses=course
        print(f"the names of the student is{name},and his cource is{course}")
    def display(self):
        print(f"the name of the student is{self.names}")
        print("the second method is been sucessfully verified")
    def coures(self):
        self.followers+=1
s1=ClassName("samyukthasrinivas","python")
print(s1.names)
print(s1.courses)
s2=ClassName("laharisrinivas","c")
print(s2.names)
print(s2.courses)
s1.display()
s2.display()
s1.coures()
s1.followers=1000
print(s1.followers)
print(s2.followers)
print(s1.coures())

