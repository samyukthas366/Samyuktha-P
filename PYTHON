import statistics
def mean_median_mode(list1):
    return [statistics.mean(list1),statistics.median(list1),statistics.mode(list1)]
print(mean_median_mode([1,2,3,4,4,5,6,6,7,7,8,8,9,9]))

import statistics
def mean_median_mode(list1):
    return statistics.mean(list1),statistics.median(list1),statistics.mode(list1)
print(mean_median_mode([1,2,3,4,4,5,6,6,7,7,8,8,9,9]))


import statistics
def mean_median_mode(list1):
    return {statistics.mean(list1), statistics.median(list1),statistics.mode(list1)}
print(mean_median_mode([1,2,3,4,4,5,6,6,7,7,8,8,9,9]))

