class numbers:
    __a=10
    _b=20
    c=30
    def display(self,d,e):
        self.__d=d
        self._e=e
        self.f=40
        print(self.__a+self._b+self.c)
    def display1(self):
        self.sumofall=self.__d+self._e+self.f
        print(self.sumofall)
ty=numbers()
ty.display(50,60)
ty.display1()




