class human(object):
    def eat(self):
        print("i want to eat food")
    def work(self):
        print("okoko")
    def sleep(self):
        print("okkkkkkkk")
class female(human):
    def work(self):
        super().work()
        print("i want to work very hard and smart to earns more sri")
class male(human):
    def sleep(self):
        super().sleep()
        print("i want to not sleep till the 1 at night because to work hard for my future carier")
male_1=male()
male_1.work()
male_1.sleep()
female_1=female()
female_1.work()
female_1.sleep()
female_1.sleep()


