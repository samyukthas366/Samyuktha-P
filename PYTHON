class student:
    __a=200
    __b=100
    __c=300
    def __init__(self,name,rollnumber,age):
        self.__name=name
        self.__rollnumber=rollnumber
        self.__age=age
        print("its an privatemethod of constructor")
    def display(self):
        print(self.__a + self.__b + self.__c)
        print("jgk")
    def __displayyy(self):
        print(self.__age)
        print(self.__rollnumber)
class branch(student):
    def anotherclasss(self):
       pass
w=student("v",123456,32)
w.display()
w._student__displayyy()
e1=branch("hope",6789000,300)
e1.display()
print(e1._student__name)
e1.anotherclasss()
e1._student__displayyy()

#####abstractmethod
from abc import ABC,abstractmethod
class student(ABC):
    name="samyukthasrinivas"
    age=20
    @abstractmethod
    def __init__(self,study,rollnumber):
        self.study=study
        self.roolnumber=rollnumber
        print(f"{self.name} is studing {self.study} in SVCET college and her age is {self.age}")
    def work(self):
        self.working="haedware"
        print(f"{self.name} is working in {self.working} company with salary of 100000 per month")
class personalwork(student):
    def __init__(self,buyingclothes,jewllery,study,rollnumber):
        super().__init__(study,rollnumber)
        self.buyingclothes=buyingclothes
        self.jewllery=jewllery
        print(f"{self.name} is earing more and more handfull of money this is the ouput of her hardwork only and blessing of her parents and god and goddess")
    def parents(self,nameofmom,nameoffather):
        self.nameofmom=nameofmom
        self.nameoffather=nameoffather
        print("okoko")
#s=student(15,76587)#we cannot create an object if there is an @abstractmethod(ABC) so object is not created

####encapsulation
class student:
    a=10
    b=20
    c=30
    def __init__(self,name,age):
        self.name=name
        self.age=age
        print(f"the student name is {self.name} and her age is {self.age}")
    def display(self):
        self.study="B.Tech"
        print("okokookokokkk")
    def diss(self):
        print("plplplp")
class barnch(student):
    def anotherclass(self,roomnumber,block):
        self.roomnumber=roomnumber
        self.block=block
        print(f"the student {self.name} is in the room {self.roomnumber}")
    def displayanother(self):
        print("okoko")
b=barnch(34543,"d")
b.display()
print(b.name)
b.anotherclass("kjdfks",345)
print(b.block)
print(b.roomnumber)


##protected
class student:
    _a=10
    _b=20
    _c=30
    def __init__(self,name,rollnumber):
        self._name=name
        self._rollnumber=rollnumber
        print("ok")
    def _display(self,marks):
        self.marks=marks
        print("ioioioioi")
    def display(self):
        print("yesyes")
        print(self._display)
class branch(student):
    def anothermethod(self):
        print(self._rollnumber)
        print(self.marks)
b1=branch("srilakshminarayana",123456)
print(b1._rollnumber)
print(b1._name)



##
class student:
    def __init__(self,name,age):
        self.__name=name
        self.__age=age
        print("okokok")
    def display(self):
        print(self.__name)
        print(self.__age)
    def __dis(self):
        print(self.__name)
s=student("sam",20)
s.display()
##s.__dis()


############POLYMORPHISM############
###operator overloadingloadddddddiiiiinnnngggg
class Complexnumber:
    def __init__(self,r,i):
        self.real=r
        self.imginary=i
    def __add__(self,other):
        return (f"{self.real+other.real} + {self.imginary+other.imginary} i" )
n1=Complexnumber(1,2)
n2=Complexnumber(3,4)
print(n1+n2)


class Addition:
    def __init__(self,a,b):
        self.a=a
        self.b=b
    def __add__(self,other):
        a=self.a+other.a
        b=self.b+other.b
        return (f"{self.a+other.a} + {self.b+other.b}")
    def __mul__(self,other):
        a=self.a*other.a
        b=self.b*other.b
        return (f"{self.a*other.a} * {self.b*other.b}")
    def __sub__(self,other):
        a=self.a-other.a
        b=self.b-other.b
        return (f"{self.a-other.b} - {self.b-other.b}")
N1=Addition(1,2)
N2=Addition(3,4)
print(N1+N2)
print(N1*N2)
print(N1-N2)
###operator overloadingloadddddddiiiiinnnngggg
class Numbers:
    def __init__(self,a,b):
        self.a=a
        self.b=b
    def __add__(self,other):
        a=self.a+other.a
        b=self.b+other.b
        return (f"{self.a+other.a} + {self.b+other.b}")
    def __sub__(self,other):
        a=self.a-other.a
        b=self.b-other.b
        return (f"{self.a-other.a} - {self.b-other.b}")
    def __mul__(self,other):
        a=self.a*other.a
        b=self.b*other.b
        return (str(self.a*other.a) + str(self.b*other.b))
    def __eq__(self,other):
        a=self.a==other.a
        b=self.b==other.b
        return (f"{self.a==other.a} and {self.b==other.b}")
    def __ge__(self,other):
        a=self.a>=other.a
        b=self.b>=other.b
        return (str(self.a>=other.a) and str(self.b>=other.b))
n1=Numbers(1,4)
n2=Numbers(1,6)
print(n1+n2)
print(n1-n2)
print(n1*n2)
print(n1==n2)
print(n1>=n2)

###method overloaddiiinnnggg
class Numbers:
    def works(self,a,b):
        print("its an 1 st method")
        self.a=a
        self.b=b
    def works(self,a,b,c):
        self.a=a
        self.b=b
        self.c=c
        print(self.a+self.b+self.c)
        print("okok")
n1=Numbers()
n1.works(1,2,0)

class Numbers:
    def works(self,*args):
        total=0
        for i in args:
            total+=i
        print(total)
n1=Numbers()
n1.works(1,2)
n1.works(3,4,8)
n1.works(1,2,3,4,5)

####method overridding
class Animals:
    def speaks(self,s,d):
        self.s=s
        self.d=d
        print("ok")
class dog(Animals):
    def speaks(self,s,d):
        super().speaks(s,d)
        print("popopop")

n=dog()
n.speaks(123,456)

class Dundermethods:
    def __init__(self,name,age):
        self.name=name
        self.age=age
    def __len__(self ):
        a=self.name
        b=self.age
        return len(a),len(b)
c=Dundermethods("samyukthasrinivas","20")
print(len(c))



class Dundermethods:
    def __init__(self,name,age):
        self.name=name
        self.age=age
    def __str__(self):
        a=self.name
        return str(a)
c=Dundermethods(3543654768979,20)
print(str(c))
print(type(c))

























    


    


