#####public access specifier
class student:
    a=10
    b=70
    c=90
    def __init__(self,name,rollnumber,age):
        self.name=name
        self.rollnumber=rollnumber
        self.age=age
        print("ok")
    def display(self):
        print(self.age)
        print(self.a+self.b+self.c)
class branch(student):
    pass
    def publicmethod(self):
        print(self.a)
        print(self.rollnumber)
s=student("sam",235467576,20)
s.display()
print(s.rollnumber)
print(s.age)
print(s.a+9)
print(s.a)
s1=branch("lahari",436576879,18)
s1.publicmethod()
print(s1.age)

##protected access specifiers
class student:
    _a=10
    _b=20
    _c=30
    def __init__(self,name,rollnumber,age):
        self._name=name
        self._rollnumber=rollnumber
        self._age=age
        print("ok")
    def _display(self,surname):
        print(self._age)
        self._surname=surname
    def _disss(self):
        print(self._surname)
class branch(student):
    def anotherclass(self):
        print(self._age)
        print(self._a+self._b+self._c)
s2=student("narayana",45465768798000000,"millions")
s2._display("housename")

