import os
def bidding_data(bidders_detailes):
    heighest_bid=0
    winner=(" ")
    for bidder in bidders_detailes:
        bidder_price=bidders_detailes[bidder]
        if(bidder_price>heighest_bid):
            heighest_bid=bidder_price
            winner=bidder
    print(f"the winner is{winner} with abid price of {heighest_bid}")
bidder_new={}
end_of_bidding_data=False
while not end_of_bidding_data:
    name=input("enter the name of biddder?")
    price=int(input("enter the amount for bidding"))
    bidder_new[name]=price
    more_bidders=input("enter more bidders? yes or no").lower()
    if(more_bidders=="no"):
        end_of_bidding=True
        find_winner(bidder_new)
