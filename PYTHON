class Instructor():
    followers=0
    def __init__(self,name,course):
        self.names=name
        self.course=course
    def display(self,name_of_the_hometown):
        print(f"the name is{self.names},and my hometown is {name_of_the_hometown}")
    def updatr_followers(self):
        self.followers+=1
s1=Instructor("sam","python")
print(s1.names)
print(s1.course)
s1.display("royalpeta")
print(s1.display("royalpeta"))


import math
class circle():
    def __init__(self,radius):
        self.radius_of_circle=radius
    def area(self):
        return math.pi*self.radius_of_circle**2
    def circumference(self):
        return 2*math.pi*self.radius_of_circle
c1=circle(10)
print(f"the area of the circle is as the {c1.area}")

######INHERITANCE####
class human():
    nose=1
    mouth=1
    eyes=2
    ears=2
    def __init__(self,name,address):
        print("i will work hard")
    def eat(self):
        print("i will eat food very good")
    def move(self):
        print("i will move to work for another place")
class male(human):
    def __init__(self,names):
        self.name=names
    def eat(self):
        super().eat()
        print("at some times i will eat more and at night i will eat less food to avoid sleep")
male_1=male("s")
print(male_1.nose)
male_1.eat()
print(male_1.mouth)

class father():
    nose=1
    mouth=1
    ears=2
    colour="white"
    moules=2
class son(father):
    def properties(self):
       colour="red"
son1=son()
print(son1.nose)
print(son1.colour)
son1.properties()
print(son1.colour)


class father:
    nose=1
    def __init__(self,name,address):
        self.names=name
        self.address=address
        print(f"my name is {name}")
    def work(self):
        print("i will work")
    def eat(self):
        print("i will eat")
class son(father):
    def properties(self):
        print("i have a bike")
    def work(self):
        super().work()
        print("i am hardware engineer")
o1=son("sri","royal")
print(o1.nose)
print(o1.eat)
o1.work()
o1.eat()


class human:
    eyes=2
    ears=2
    def __init__(self):
        self.nose=1
        self.mouth=1
    def work(self):
        print("the nose can sense the smell and mouth sense the taste")
    def eat(self):
        print("with help of the mouth we can eat food")
class female(human):
    def work(self):
        super().work()
        print("ok0")
female_1=female()
print(female.eyes)
female_1.work()
print(female_1.nose)



class human:
    eyes=2
    def __init__(self):
        self.angry="more"
        self.flirting="more and more"
        print(f"the angry of the human is {self.angry}")
    def work(self):
        print("i will work")
class male(human):
    def __init__(self):
        super().__init__()
        self.onetype="malee"
        self.another="feemale"
    def work(self):
        super().work()
        print("i will will work more smarter and harder")
male_1=male()
male_1.work()
print(male_1.angry)
print(male_1.another)


class human:
    eyes=1
    def __init__(self,num_heart):
        self.nose=1
        self.mouth=1
        self.num_heart=num_heart
    def work(self):
        print("i will work smart")
    def eat(self):
        print(f"i can eat by using {self.mouth} mouth")
class female(human):
    def __init__(self,name,heart):
        super().__init__(heart)
        self.name=name
    def work(self):
        print("i will work more hard and god will always give me good blessings ")
female_1=female("payal",1)
print(female_1.name)
print(female_1.eyes)
print(female_1.nose)
female_1.work()
print(female_1.num_heart)


class human:
    eyes=2
    def __init__(self,names,address):
        self.names=names
        self.address=address
        self.mouth=1
        self.teeth=32
    def display(self):
        print("it will display")
    def eat(self):
        print("i can eat with my {self.mouth} mouth")
class female(human):
    def __init__(self,lastname,names,address):
        super().__init__(names,address)
        self.lastname=lastname
        print(f"the{lastname} is the last name of the samyuktha")
female_1=female("penumueri","samyukthasrinivas","royalpeta")
print(female_1.eyes)
print(female_1.names)
print(female_1.address)




