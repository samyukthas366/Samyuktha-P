class human(object):
    def eat(self):
        print("i want to eat food")
class female(human):
    def work(self):
        print("i want to work very hard and smart to earns more sri")
class male(human):
    def sleep(self):
        print("i want to not sleep till the 1 at night because to work hard for my future carier")
male_1=male()
male_1.eat()
male_1.sleep()
female_1=female()
female_1.eat()
female_1.work()

