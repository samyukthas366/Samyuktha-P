###########multilevel inheritance#############
$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
class Sam:
    colour="blue"
    def work(self):
        print("i will work by getting starting salary of  my carier at first company in college campusselection   the 80000 per month in hard ware")
class Lahari(Sam):
    def eat(self):
        print("i will work by getting starting salary of the 100000 per month in hard ware")
class Rama(Lahari):
    def eat(self):
        super().eat()
        super().work()
        print("i will work by getting starting salary of the 90000 per month in hard ware")
rama_1=Rama()
rama_1.eat()
print(rama_1.colour)
rama_1.eat()
rama_1.work()


##multiple inheritance by using only init method
class Requirements:
    def __init__(self,colour,frabics):
        self.colour=colour
        self.frabics=frabics
        self.kundans="flowerkundans"
class Design(Requirements):
    def __init__(self,croptop,skirt):
        self.croptop=croptop
        self.skirt=skirt
        print("design part is beeen conformed")
class Implementation(Design):
    def __init__(self,overalllook,colour,frabics,croptop,skirt):
        Requirements.__init__(self,colour,frabics)
        Design.__init__(self,croptop,skirt)
        self.overalllook=overalllook
        print("implementation is also successfully completed")
imple_1=Implementation("excellent","blue","cotton","red","cream")
print(imple_1.colour)
print(imple_1.frabics)
print(imple_1.overalllook)


###doubt
class Requirements(object):##object class (or) derived class (or)parent (or) super class
    def __init__(self,colour,frabics):
        self.colour=colour
        self.frabics=frabics
        self.kundans="flowerkundans"
class Design(Requirements):#these are are what class
    def __init__(self,croptop,skirt):
        self.croptop=croptop
        self.skirt=skirt
        print("design part is beeen conformed")
class Implementation(Design):
    def __init__(self,overalllook,colour,frabics,croptop,skirt):
        Requirements.__init__(self,colour,frabics)
        Design.__init__(self,croptop,skirt)
        self.overalllook=overalllook
        print("implementation is also successfully completed")
imple_1=Implementation("excellent","blue","cotton","red","cream")
print(imple_1.colour)
print(imple_1.frabics)
print(imple_1.overalllook)

