#########ENCAPSULATION##########
###############################
class dog:
    legs=4
    def __init__(self,speaks1):
        self._speaks=speaks1#protected
        print(f"the animal dog  always says {speaks1}")
    def eats(self, food1):
        self._food = food1
        print(f"the cat always eats {food1}")
class cat(dog):
    def __init__(self,speaks2,speaks1):
        dog.__init__(self,speaks1)
        self.speaks=speaks2
        self.hands=2
        print(f"the cat always says {speaks2}")
    def eats(self,food2,food1):
        super().eats(food1)
        self.food=food2
        print(f"the cat always eats {food2}")
class monkey(dog):
    def __init__(self,speak3,speaks1):
        dog.__init__(self,speaks1)
        self.speakss=speak3
        self.hands=2
        print(f"the cat always says {speak3}")
    def eatsss(self,food3):
        self.food=food3
        print(f"the cat always eats {food3}")
c1=monkey("mwowwww","kkkkkk")
c1.eatsss("veg briyani")
c=cat("meow","bowbow")
c.eats("meat","milk")



class dog:
    legs=4
    def __init__(self,speaks1):
        self.__speaks=speaks1#protected
        print(f"the animal dog  always says {speaks1}")
    def eats(self, food1):
        self.__food = food1
        print(f"the cat always eats {food1}")
class cat(dog):
    def __init__(self,speaks2,speaks1):
        dog.__init__(self,speaks1)
        self.speaks=speaks2
        self.hands=2
        print(f"the cat always says {speaks2}")
    def eats(self,food2,food1):
        super().eats(food1)
        self.food=food2
        print(f"the cat always eats {food2}")
class monkey(dog):
    def __init__(self,speak3,speaks1):
        dog.__init__(self,speaks1)
        self.speakss=speak3
        self.hands=2
        print(f"the cat always says {speak3}")
    def eatsss(self,food3):
        self.food=food3
        print(f"the cat always eats {food3}")
c1=monkey("mwowwww","kkkkkk")
c1.eatsss("veg briyani")
c=cat("meow","bowbow")
c.eats("meat","milk")



class bankaccount:
    name_of_account="samyuktha"
    def __init__(self,balance):
        self._balance=balance
        print(f"the present balance of the samyuktha is {self._balance}")
    def deposite(self,amount):
        self._balance+=amount
        self.bonus=10000
    def withdrawal(self,amounttt):
        self._balance-=amounttt
        print(f"the total present balance of the account holder samyuktha is {self._balance}")
class lahari(bankaccount):
    def __init__(self,balance_of_lahari,balance):
        super().__init__(balance)
        self.balance_of_lahari=balance_of_lahari
        print(f"the balance of the lahari is {self.balance_of_lahari}")
    def deposit(self):
        print(f"the balance of the lahari is {self.balance_of_lahari}")
a=bankaccount(700000)
a.deposite(100000)
print(a._balance)
y=lahari(300000,2000000)
print(y._balance)



