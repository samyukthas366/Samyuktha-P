class sam:
    def __init__(self,gold,car):
        self._gold=gold
        self._car=car
        self._home=1
        print("all the properties of sam can be used by the its own sub classes only")
    def property(self):
        self.money=100000
        print("these all money can be also used by her parents and sister")
class parentslah(sam):
    def eatss(self,food):
        self.food=food
        print(f"the food is as {self.food}")
p=parentslah("1kg","blueblack")
