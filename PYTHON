bill=0
if(a>10 and a<15):
    print("you should pay the 1/4 amount")
    bill=100
elif(10<20):
    print("you should pay the half amount")
    bill=230
else:
    print("you should pay full amount")
    bill=500
print(f"you need to pay this much amount ,{bill}")
