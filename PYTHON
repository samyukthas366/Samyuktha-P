###########multiple inheritance
class father:
    nose=1
    def __init__(self,surname):
        self.surname=surname
        self.colour="white"
        print("it is been from the fatherclass")
    def work(self):
        print("always working hard for my children")
class mother:
    eyes=2
    def __init__(self,hair):
        self.hair=hair
        self.height=5.3
        print("it is all about the motherclass")
    def work(self):
        print("i will always cook best food for my children and take care of family")
class child(father,mother):
    fingers=6
    def __init__(self,name,surname,hair):
        father.__init__(self,surname)
        mother.__init__(self,hair)
        self.name=name
        self.weight=40
        print("it is from the child class when the object is been created then the method is exicuted")
    def work(self):
        print("i am an hardware engineering student")
child_1=child("samsrinivas","penumuri","black")
child_1.work()
mother.work(child_1)
father.work(child_1)
print(child_1.surname)
print(child_1.hair)
print(child_1.weight)
print(child_1.name)
mother.work(child_1)

###########only the class varaiables inheritd from parents to the child


class rama:
    eyes=2
    colour="blue"
    print("it is an ramasclass")
class sita:
    nose=1
    hair="curle"
    print("it is an sitasclass")
class kusha(rama,sita):
    print("it is an kusha class and all the properties are been inherted from the mom and dad")
kusha_1=kusha()
print(kusha_1.eyes)
print(kusha_1.hair)
print(kusha_1.nose)
print(kusha_1.colour)
kusha_2=kusha()


###just an attributes or variables in by using the single inheritance
class rama:
    colour="blue"
    height=5.4
    print(f"rama is in {colour} and height of rama is {height}")
class lava(rama):
    weight=40
    print("if is all qualities inherits from the rama whose id lava's father")
lava_1=lava()
print(lava_1.colour)
print(lava_1.height)
print(lava_1.weight)


####single inheritance by using the init constructor by using the super keyword
class rama:
    colour="blue"
    def __init__(self,height,weight):
        self.height=height
        self.weight=weight
        self.eye_ball="brown"
        print("it will be exicuted when only the object is been created")
class lava(rama):
    fingers=6
    def __init__(self,surname,heights,weights):
        super().__init__(heights,weights)
        self.surname=surname
        self.skintone="blueishblack"
lava_1=lava("kosala",5.6,54)
print(lava_1.colour)
print(lava_1.fingers)
print(lava_1.height)
print(lava_1.weight)
print(lava_1.skintone)


####it is an code single inheritance using the init constructor with two methos by using the supwe keyword
class rama:
    colour="blue"
    def __init__(self,surname,properties):
        self.surname=surname
        self.properties=properties
        self.bike=2
    def work(self):
        print("i will rule the kingdom of kosala")
class lava(rama):
    fingers=6
    def __init__(self,car,surname,properties):
        self.car=car
        super().__init__(surname,properties)
    def work(self):
        super().work()
        print("i will will help the people in my kingdom")
lava_1=lava("BMW","kosala",1000000)
print(lava_1.car)
print(lava_1.surname)
print(lava_1.colour)
print(lava_1.properties)
lava_1.work()


###it is an code single inheritance using the init constructor with two methos by using the supwe keyword by using attributes in second metthod also and inheritting them in child class
class rama:
    colour="blue"
    def __init__(self,surname,properties):
        self.surname=surname
        self.properties=properties
        self.bike=2
    def work(self):
        self.towns=200
        print("i will rule the kingdom of kosala")
class lava(rama):
    fingers=6
    def __init__(self,car,surname,properties):
        self.car=car
        super().__init__(surname,properties)
    def work(self):
        self.villages_ruling=10000
        super().work()
        print("i will will help the people in my kingdom")
lava_1=lava("BMW","kosala",1000000)
print(lava_1.car)
print(lava_1.surname)
print(lava_1.colour)
print(lava_1.properties)
lava_1.work()
print(lava_1.villages_ruling)
print(lava_1.towns)



############MULTIPLE INHERITANCE#########
class rama:
    colour="blue"
    height=5.7
    def __init__(self,surname):
        self.surname=surname
        self.properties=100000
        print("the object is been created so that the init constructor is been exicuted")
class sita:
    weight=54
    def __init__(self,eye_ball):
        self.eye_ball=eye_ball
        self.looks="beautiful"
        print(f"sita is having all the properties from rama are given as {self.surname},{self.properties}")
class lava(rama,sita):
    def __init__(self,horoscope,surname,eye_ball):
        rama.__init__(self,surname)
        sita.__init__(self,eye_ball)
        self.horoscope=horoscope
        self.study="ramayana"
lava_1=lava("all","kosala","black")
print(lava_1.eye_ball)
print(lava_1.eye_ball)

######multiple methods in the multiple inheritance
class Rama:
    colour="blue"
    height=5.7
    def __init__(self,surname):
        self.surname=surname
        self.properties=100000
        print("the object is been created so that the init constructor is been exicuted")
    def work(self):
        print("i will rule the kingdom of765856 kosala")
class Sita:
    weight=54
    def __init__(self,eye_ball):
        self.eye_ball=eye_ball
        self.looks="beautiful"
        print(f"sita is having all the properties from rama are given as {self.surname},{self.properties}")
    def work(self):
        self.houses=4
        print("i worked hard for the house building")
class Lava(Rama,Sita):
    def __init__(self,horoscope,surname,eye_ball):
        Rama.__init__(self,surname)
        Sita.__init__(self,eye_ball)
        self.horoscope=horoscope
        self.study="ramayana"
    def work(self):
        print("i will rule the kingdom of kosala")
lava_1=Lava("all","kosala","black")
print(lava_1.eye_ball)
print(lava_1.eye_ball)
lava_1.work()
Rama.work(lava_1)
Sita.work(lava_1)

