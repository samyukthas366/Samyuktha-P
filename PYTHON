def a(b):
    c=f"sum is{sum(b.values())}"
    return c
print(a({1:324,2:345,3:456}))

def a(*b):
    c=f"sum is {sum(b)}"
    return c
print(a(2,3,4,5,5,66,7,7))

def a(b):
    c=f"sum is {sum(b.values())}"
    return c
print(a({1:345,"sam":123,"lah":345}))

def a(**b):
    c=f"sum is {(sum(b.values())*3)}"
    return c
print(a(s=1,d=2,j=3,t=9))


def numbers(*a):
    print(f"sum is {sum(a)}")
numbers(1,2,3,4,54)

def numbers(a,b):
    print(f"the sum is {a+b}")
numbers(1,2)

def multiplication(a,b):
    print(f"the multiplication of the numbers is given by the sum is {a*b}")
multiplication(2,3)

def division(a,b):
    print(f"the division is given by {a/b}")
division(2,3)



def names(a,b):
    print(f"hello {a}")
    print(f"hi {b}")
names("samsrinivas","lahsrinivas")


def data(name,age):
    print(f"my name is{name} and my age is {age}")
data("srinivas",20)

def data(age,name="samyu"):
    print(f"my name is{name} and the my name is{age}")
data(20)

def data(**a):
    print(f"my name is{a}")
data(name="samyu",age=20)


def data(a):
    print(f" my name is {a}")
data(a="samyu")


