def new_data(name,age,roll_no,cource):
    new_dict={}
    new_dict["Name"]=name
    new_dict["Age"]=age
    new_dict["Roll_no"]=roll_no
    new_dict["Cource"]=cource
    q.update(new_dict)
new_data("jay sri radha krishna",20,2354,"python")
print(q)


def a(*age):
    d={}
    d["Age"]=age
    d["Age1"]=age
    d["Age2"]=age
    d["Age3"]=age
    q.update(d)
a(23,34,56,67)
print(q)

