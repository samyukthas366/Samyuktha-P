class rama:
    father="dasaradha"
    def __init__(self,surname,age):
        self.surname=surname
        self.age=age
        print(f"the surname of the rama is {self.surname} and  his father is {self.father}")
    def work(self,town):
        self.town=town
        print(f"the town of the rama is {self.town}")
class sita(rama):
    def __init__(self,veg,age,land):
        super().__init__(surname,age)

class rama:
    father="dasharadha maharaja"
    mother="kouslya"
    def __init__(self,surname,kingdom):
        self.surname=surname
        self.kingdom=kingdom
        self.daughters=0
        print("ok this print statement is been exicuted only when there is a calling of the object")
        print(f"rama's father name is {self.father},and mother name is {self.mother}")
        print(f"rama is having {self.daughters},and his kingdom is {self.kingdom}")
    def work(self,ruling):
        self.rulin=ruling
        self.land=1000
        print("this is the work is called so this one is printed")
        print(f"rama is having an kingdom is {self.kingdom},and his land is {self.land}")
    def eat(self,plantfood):
        self.plantfood=plantfood
        self.fruits="apple,oranges,bananas"
class lavakusha(rama):
    colour="white"
    def __init__(self,agrland,homes,surname,kingdom):
        super().__init__(surname,kingdom)
        self.agrland=agrland
        self.homes=homes
        print(f"this is been printed only {self.agrland} when the lavakusha class object is been created")
    def eat(self,junkfood,plantfood):
        super().eat(plantfood)
        self.junkfood=junkfood
        self.juices=2
        print("okokokoko")
l=lavakusha(20,30,"raghuvamsham","kosala")
print(l.agrland)
print(l.homes)
print(l.colour)
l.eat("biscuits","leafyvegitables")
print(l.juices)
l.agrland=245
print(l.agrland)
l.junkfood="pizza"
print(l.junkfood)
l.kingdom="kkkkkkkkkkkkk"
print(l.kingdom)


