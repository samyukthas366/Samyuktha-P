def year(a,b,c):
    for i in year():
        if(a/4==0):
            return("it is an leap year")
        else:
            return("not leap year")
    else:
        return("ok")
print(year(2024,2025,2020))
def a(*year):
    for i in year:
       if(i%400==0):
           return("its an leap year")
       else:
           return("not an leap year")
    else:
       print("ok")
print(a(2000,2400,2025,2040))'''

