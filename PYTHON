def new_data(name,age,roll_no,cource):
    new_dict={}
    new_dict["Name"]=name
    new_dict["Age"]=age
    new_dict["Roll_no"]=roll_no
    new_dict["Cource"]=cource
    q.update(new_dict)
new_data("jay sri radha krishna",20,2354,"python")
print(q)


