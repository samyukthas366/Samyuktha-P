class car():
    wheel=9
    color="blue"
    def __init__(self,name_of_car,cost_of_car,song_systems):
        self.names=name_of_car
        self.costs=cost_of_car
        self.songs=song_systems
        print("ok the object is been created so its printing")
ca1=car("oaddy",600000,3)
print(ca1)
print(ca1.songs)
ca2=car("benzzz",8000000,6)
print(ca2.names)
ca2.names="benzzzzzzzzz"
print(ca2.names)

