class bus():
    wheels=9
    seats=50
    color="black"
    def __init__(self,place,laguageplace):
        self.plaes=place
        self.laguageplace=laguageplace
    def __init__(self,ladders,lights):
        self.ladders=ladders
        self.lights=lights
c1=bus(2,5)
print(c1.ladders)
print(c1.lights)
print(type(c1))
print(c1)


