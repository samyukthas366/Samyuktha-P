h={"sam":99,
   "lah":89,
   "srinivas":79,
   "sdgd":46
}
grade={}
for student in h:
    marks=h[student]
    if(marks>90):
        grade[student]="A+"
    elif(marks>80):
        grade[student]="A"
    elif(marks>70):
        grade[student]="B+"
    elif(marks>60):
        grade[student]="B"
    elif(marks>50):
        grade[student]="C"
    elif(marks>40):
        grade[student]="D"
    else:
        grade[student]="fail"
print(grade)


