class people(object):
    people1="white"
    people2="black"
    def __init__(self,legs,hands):
        self.legs=legs
        self.hands=hands
        print(f"the human is having an {legs} legs and {hands} hands")
class teacher(people):
    fingers=5
    def __init__(self,eyes,legs,hands):
        people.__init__(self,legs,hands)
        self.eyes=eyes
        self.nose=1
        print("okkkkk")
class student(people):
    mouth=1
    def __init__(self,legs,hands):
        people.__init__(self,legs,hands)
        print("i will learn and get a good job ")
st=student(2,2)
print(st.people1)
print(st.legs)
print(st.hands)
te=teacher(2*3,4*4,6*1)
print(te.people1)
print(te.eyes)
print(te.legs)


########################
#####HYBRID INHERITANCE###
##########################
class A(object):
    def eat(self):
        print("i eat oranges everyday")
    def work1(self):
        print("i will go to gim 0000000for my workout")
class B(A):
    def work(self):
        super().work1()
        print("i will go to gim for my workout")
class C:
    def sleep(self):
        print("i will sleep at 12o clock everyday")
class D(B,C):
    def play(self):
        print("i will play games in our garden")
d1=D()
d1.play()
d1.sleep()
d1.work()
d1.eat()
